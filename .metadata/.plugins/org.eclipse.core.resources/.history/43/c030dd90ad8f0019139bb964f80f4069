public class removeA {
	
	functionality f;
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
	}
	@BeforeEach
	void setUp() {
		f = new functionality();
	}
	@Test
	void test() {
		
		assertEquals("BCD",f.removea("ABCD"));
		
	}
	@Test
	void test() {
		
		
		assertEquals("CD",f.removea("AACD"));
		
	}
	@Test
